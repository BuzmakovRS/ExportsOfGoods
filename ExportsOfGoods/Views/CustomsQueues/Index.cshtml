@model IEnumerable<ExportsOfGoods.Models.CustomsQueue>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Customs.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Parti.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeBegInsp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeEndInsp)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Customs.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Parti.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeBegInsp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeEndInsp)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
